{
  "name": "asfop/constants",
  "description": "An easy-to-use enumeration expansion package helps you manage enumerations in projects more easily",
  "license": "MIT",
  "keywords": [
    "php",
    "enum",
    "constants"
  ],
  "authors": [
    {
      "name": "xxx",
      "email": "xxx@gmail.com"
    }
  ],
  "require": {
    "ext-json": "*",
    "php": ">=7.1|>=8.0|>=8.1"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Asfop\\Constants\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Asfop\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs-fix": "./vendor/bin/phpcbf --standard=PSR2 --colors src/"
  },
  "require-dev": {
    "phpunit/phpunit": "8.5.x-dev",
    "sebastian/phpcpd": "^4.1",
    "squizlabs/php_codesniffer": "4.0.x-dev"
  }
}
